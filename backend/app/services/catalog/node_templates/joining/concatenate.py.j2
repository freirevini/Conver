{# 
  Concatenate Template
  Appends rows from multiple DataFrames
#}
def node_{{ node_id }}_concatenate(
    input_0: pd.DataFrame,
    input_1: pd.DataFrame,
    *,
    {% if settings.skip_duplicates %}
    skip_duplicates: bool = {{ settings.skip_duplicates | lower }},
    {% else %}
    skip_duplicates: bool = False,
    {% endif %}
    {% if settings.ignore_types %}
    ignore_column_types: bool = {{ settings.ignore_types | lower }},
    {% else %}
    ignore_column_types: bool = False,
    {% endif %}
) -> pd.DataFrame:
    """
    {{ node_name }}
    KNIME Node ID: {{ node_id }}
    Factory: {{ factory_class }}
    
    Concatenates (appends) rows from two DataFrames.
    """
    # Concatenate DataFrames
    df = pd.concat([input_0, input_1], ignore_index=True)
    
    if skip_duplicates:
        df = df.drop_duplicates()
    
    logger.debug(f"Concatenated: {len(input_0)} + {len(input_1)} -> {len(df)} rows")
    return df
